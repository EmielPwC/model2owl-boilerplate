cccev:AdminUnit a owl:Class ;
    skos:definition "A detailed administrative unit organized in a hierarchy."@en ;
    skos:prefLabel "Admin unit"@en .

cccev:adminUnit a owl:ObjectProperty ;
    skos:definition "The adminUnit relationship links an Address with the Administrative Unit class. (locn:Address -> clv:AdminUnit (+clv:adminUnit))"@en ;
    skos:prefLabel "Admin unit"@en .

cccev:code a owl:ObjectProperty ;
    skos:definition "The classification of the administrative unit. (clv:AdminUnit -> clv:code (+clv:code))"@en ;
    skos:prefLabel "Code"@en .

cccev:coordinates a owl:DatatypeProperty ;
    skos:definition "A list of geographic coordinates that define the extent of the Geometry. (locn:Geometry)"@en ;
    skos:prefLabel "Coordinates"@en .

cccev:crs a owl:DatatypeProperty ;
    skos:definition "An identifier for the coordinate reference system. (locn:Geometry)"@en ;
    skos:prefLabel "Crs"@en .

cccev:domicile a owl:ObjectProperty ;
    skos:definition "The place that the Person treats as permanent home. (cpv:Person -> locn:Address (+cpv:domicile))"@en ;
    skos:prefLabel "Domicile"@en .

cccev:geometryType a owl:ObjectProperty ;
    skos:definition "The classification of the Geometry. (locn:Geometry -> clv:geometryType (+clv:geometryType))"@en ;
    skos:prefLabel "Geometry type"@en .

cccev:latitude a owl:DatatypeProperty ;
    skos:definition "The geographic coordinate that specifies the north / south position of the Geomerty on the Earth's surface. (locn:Geometry)"@en ;
    skos:prefLabel "Latitude"@en .

cccev:level a owl:ObjectProperty ;
    skos:definition "The level of the administrative unit in the hierarchy. (clv:AdminUnit -> clv:level (+clv:level))"@en ;
    skos:prefLabel "Level"@en .

cccev:longitude a owl:DatatypeProperty ;
    skos:definition "The geographic coordinate that specifies the east / west position of the Geometry on the Earth's surface. (locn:Geometry)"@en ;
    skos:prefLabel "Longitude"@en .

cccev:registeredAddress a owl:ObjectProperty ;
    skos:prefLabel "Registered address"@en .

dcterms:Location a owl:Class ;
    skos:definition "An identifiable geographic place or named place."@en ;
    skos:prefLabel "Location"@en .

geo:asGML a owl:DatatypeProperty ;
    skos:definition "The expression of the Geometry in Geography Markup Language. (locn:Geometry)"@en ;
    skos:prefLabel "As g m l"@en .

geo:asWKT a owl:DatatypeProperty ;
    skos:definition "An expression of the Geometry in WKT, the Well-Known Text markup language. (locn:Geometry)"@en ;
    skos:prefLabel "As w k t"@en .

rdfs:Resource a owl:Class ;
    skos:definition "Anything that can be described."@en ;
    skos:prefLabel "Resource"@en .

rdfs:seeAlso a owl:DatatypeProperty ;
    skos:definition "A reference in the form of a Uniform Resource Identifier to the Location. (dct:Location)"@en ;
    skos:prefLabel "See also"@en .

locn:Address a owl:Class ;
    skos:definition "A spatial object that in a human-readable way identifies a fixed location."@en ;
    skos:prefLabel "Address"@en .

locn:Geometry a owl:Class ;
    skos:definition "A shape or form of a Location."@en ;
    skos:prefLabel "Geometry"@en .

locn:address a owl:ObjectProperty ;
    skos:definition "Associates any Resource with the corresponding Address. (rdfs:Resource -> locn:Address (+locn:address)) The Address of a Public Organization. (cpov:PublicOrganisation -> locn:Address (+locn:address))"@en ;
    skos:prefLabel "Address"@en .

locn:addressArea a owl:DatatypeProperty ;
    skos:definition "The name of a geographic area that groups Addresses. (locn:Address)"@en ;
    skos:prefLabel "Address area"@en .

locn:addressID a owl:DatatypeProperty ;
    skos:definition "A globally unique identifier for each instance of an Address. (locn:Address)"@en ;
    skos:prefLabel "Address i d"@en .

locn:adminUnitL1 a owl:DatatypeProperty ;
    skos:definition "The name of the uppermost level of the address, almost always a country. (locn:Address)"@en ;
    skos:prefLabel "Admin unit l1"@en .

locn:adminUnitL2 a owl:DatatypeProperty ;
    skos:definition "The name of a secondary level/region of the address, usually a county, state or other such area that typically encompasses several localities. (locn:Address)"@en ;
    skos:prefLabel "Admin unit l2"@en .

locn:fullAddress a owl:DatatypeProperty ;
    skos:definition "The complete address written as a string. (locn:Address)"@en ;
    skos:prefLabel "Full address"@en .

locn:geographicName a owl:DatatypeProperty ;
    skos:definition "A textual description for a Location. (dct:Location)"@en ;
    skos:prefLabel "Geographic name"@en .

locn:geometry a owl:ObjectProperty ;
    skos:definition "Associates any Resource with the corresponding Geometry. (rdfs:Resource -> locn:Geometry (+locn:geometry))"@en ;
    skos:prefLabel "Geometry"@en .

locn:location a owl:ObjectProperty ;
    skos:definition "Associates any Resource with the corresponding Location. (rdfs:Resource -> dct:Location (+locn:location))"@en ;
    skos:prefLabel "Location"@en .

locn:locatorDesignator a owl:DatatypeProperty ;
    skos:definition "A number or sequence of characters that uniquely identifies the locator within the relevant scope. (locn:Address)"@en ;
    skos:prefLabel "Locator designator"@en .

locn:locatorName a owl:DatatypeProperty ;
    skos:definition "Proper noun(s) applied to the real world entity identified by the locator. (locn:Address)"@en ;
    skos:prefLabel "Locator name"@en .

locn:poBox a owl:DatatypeProperty ;
    skos:definition "A location designator for a postal delivery point at a post office, usually a number. (locn:Address)"@en ;
    skos:prefLabel "Po box"@en .

locn:postCode a owl:DatatypeProperty ;
    skos:definition "The code created and maintained for postal purposes to identify a subdivision of addresses and postal delivery points. (locn:Address)"@en ;
    skos:prefLabel "Post code"@en .

locn:postName a owl:DatatypeProperty ;
    skos:definition "A name created and maintained for postal purposes to identify a subdivision of addresses and postal delivery points. (locn:Address)"@en ;
    skos:prefLabel "Post name"@en .

locn:thoroughfare a owl:DatatypeProperty ;
    skos:definition "The name of a passage or way through from one location to another. (locn:Address)"@en ;
    skos:prefLabel "Thoroughfare"@en .

person:countryOfBirth a owl:ObjectProperty ;
    skos:definition "The country in which the Person was born. (cpv:Person -> dct:Location (+person:countryOfBirth))"@en ;
    skos:prefLabel "Country of birth"@en .

person:countryOfDeath a owl:ObjectProperty ;
    skos:definition "The country in which the Person died. (cpv:Person -> dct:Location (+person:countryOfDeath))"@en ;
    skos:prefLabel "Country of death"@en .

person:placeOfBirth a owl:ObjectProperty ;
    skos:definition "The Location where the Person was born. (cpv:Person -> dct:Location (+person:placeOfBirth))"@en ;
    skos:prefLabel "Place of birth"@en .

person:placeOfDeath a owl:ObjectProperty ;
    skos:definition "The Location where the Person died. (cpv:Person -> dct:Location (+person:placeOfDeath))"@en ;
    skos:prefLabel "Place of death"@en .

