cccev:adminUnit rdfs:domain locn:Address ;
    rdfs:range cccev:AdminUnit .

cccev:coordinates rdfs:domain locn:Geometry ;
    rdfs:range xsd:string .

cccev:crs rdfs:domain locn:Geometry ;
    rdfs:range xsd:anyURI .

cccev:domicile rdfs:domain cccev:Person ;
    rdfs:range locn:Address .

cccev:latitude rdfs:domain locn:Geometry ;
    rdfs:range xsd:string .

cccev:longitude rdfs:domain locn:Geometry ;
    rdfs:range xsd:string .

cccev:registeredAddress rdfs:domain rdfs:Resource ;
    rdfs:range locn:Address .

geo:asGML rdfs:domain locn:Geometry ;
    rdfs:range rdfs:Literal .

geo:asWKT rdfs:domain locn:Geometry ;
    rdfs:range rdfs:Literal .

rdfs:seeAlso rdfs:domain dcterms:Location ;
    rdfs:range xsd:anyURI .

locn:address rdfs:domain [ a owl:Class ;
            owl:unionOf ( rdfs:Resource cccev:PublicOrganisation ) ] ;
    rdfs:range locn:Address .

locn:addressArea rdfs:domain locn:Address ;
    rdfs:range rdf:langString .

locn:addressID rdfs:domain locn:Address ;
    rdfs:range rdfs:Literal .

locn:adminUnitL1 rdfs:domain locn:Address ;
    rdfs:range rdf:langString .

locn:adminUnitL2 rdfs:domain locn:Address ;
    rdfs:range rdf:langString .

locn:fullAddress rdfs:domain locn:Address ;
    rdfs:range rdf:langString .

locn:geographicName rdfs:domain dcterms:Location ;
    rdfs:range rdf:langString .

locn:geometry rdfs:domain rdfs:Resource ;
    rdfs:range locn:Geometry .

locn:location rdfs:domain rdfs:Resource ;
    rdfs:range dcterms:Location .

locn:locatorDesignator rdfs:domain locn:Address ;
    rdfs:range rdfs:Literal .

locn:locatorName rdfs:domain locn:Address ;
    rdfs:range rdf:langString .

locn:poBox rdfs:domain locn:Address ;
    rdfs:range rdfs:Literal .

locn:postCode rdfs:domain locn:Address ;
    rdfs:range rdfs:Literal .

locn:postName rdfs:domain locn:Address ;
    rdfs:range rdf:langString .

locn:thoroughfare rdfs:domain locn:Address ;
    rdfs:range rdf:langString .

person:countryOfBirth rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:countryOfDeath rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:placeOfBirth rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:placeOfDeath rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

cccev:code a owl:Class ;
    rdfs:domain cccev:AdminUnit ;
    rdfs:range skos:Concept ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue cccev:code ;
            owl:onProperty skos:inScheme ] .

cccev:geometryType a owl:Class ;
    rdfs:domain locn:Geometry ;
    rdfs:range skos:Concept ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue cccev:geometryType ;
            owl:onProperty skos:inScheme ] .

cccev:level a owl:Class ;
    rdfs:domain cccev:AdminUnit ;
    rdfs:range skos:Concept ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue cccev:level ;
            owl:onProperty skos:inScheme ] .

