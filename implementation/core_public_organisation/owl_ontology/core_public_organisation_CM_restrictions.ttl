cccev:contactPoint rdfs:domain cccev:PublicOrganisation ;
    rdfs:range cccev:ContactPoint .

cccev:hasFormalFramework rdfs:domain org:ChangeEvent ;
    rdfs:range cccev:ReferenceFramework ;
    owl:inverseOf cccev:implements .

cccev:logo rdfs:domain cccev:PublicOrganisation ;
    rdfs:range cccev:ImageObject .

cccev:openingHours rdfs:domain cccev:ContactPoint ;
    rdfs:range time:TemporalEntity .

cccev:specialOpeningHoursSpecification rdfs:domain cccev:ContactPoint ;
    rdfs:range time:TemporalEntity .

dcterms:description rdfs:domain [ a owl:Class ;
            owl:unionOf ( cccev:PublicOrganisation org:ChangeEvent time:TemporalEntity ) ] ;
    rdfs:range rdf:langString .

dcterms:identifier rdfs:domain cccev:ReferenceFramework ;
    rdfs:range rdfs:Literal .

dcterms:spatial rdfs:domain cccev:PublicOrganisation ;
    rdfs:range skos:Concept .

skos:altLabel rdfs:domain cccev:PublicOrganisation ;
    rdfs:range rdf:langString .

skos:prefLabel rdfs:domain cccev:PublicOrganisation ;
    rdfs:range rdf:langString .

adms:identifier rdfs:domain cccev:PublicOrganisation ;
    rdfs:range adms:Identifier .

locn:address rdfs:domain cccev:PublicOrganisation ;
    rdfs:range locn:Address .

org:changedBy rdfs:domain cccev:PublicOrganisation ;
    rdfs:range org:ChangeEvent ;
    owl:inverseOf org:originalOrganization .

org:hasMember a owl:AsymmetricProperty ;
    rdfs:domain cccev:PublicOrganisation ;
    rdfs:range cccev:PublicOrganisation ;
    owl:inverseOf org:memberOf .

org:hasUnit rdfs:domain cccev:PublicOrganisation ;
    rdfs:range org:OrganizationalUnit ;
    owl:inverseOf org:unitOf .

org:resultedFrom rdfs:domain cccev:PublicOrganisation ;
    rdfs:range org:ChangeEvent ;
    owl:inverseOf org:resultingOrganization .

org:subOrganizationOf a owl:AsymmetricProperty ;
    rdfs:domain org:Organization ;
    rdfs:range org:Organization ;
    owl:inverseOf org:hasSubOrganization .

foaf:homepage rdfs:domain cccev:PublicOrganisation ;
    rdfs:range foaf:Document .

xhtml:next a owl:AsymmetricProperty ;
    rdfs:domain cccev:PublicOrganisation ;
    rdfs:range cccev:PublicOrganisation ;
    owl:inverseOf xhtml:prev .

prov1:startedAtTime rdfs:domain org:ChangeEvent ;
    rdfs:range xsd:dateTime .

cccev:frequency a owl:Class ;
    rdfs:domain time:TemporalEntity ;
    rdfs:range skos:Concept ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue cccev:frequency ;
            owl:onProperty skos:inScheme ] .

cccev:implements rdfs:domain cccev:ReferenceFramework ;
    rdfs:range org:ChangeEvent .

at-voc:atu-type a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue at-voc:atu-type ;
            owl:onProperty skos:inScheme ] .

org:classification a owl:Class ;
    rdfs:domain cccev:PublicOrganisation ;
    rdfs:range skos:Concept ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue org:classification ;
            owl:onProperty skos:inScheme ] .

org:hasSubOrganization rdfs:domain org:Organization ;
    rdfs:range org:Organization .

org:memberOf rdfs:domain cccev:PublicOrganisation ;
    rdfs:range cccev:PublicOrganisation .

org:originalOrganization rdfs:domain org:ChangeEvent ;
    rdfs:range cccev:PublicOrganisation .

org:purpose a owl:Class ;
    rdfs:domain cccev:PublicOrganisation ;
    rdfs:range skos:Concept ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue org:purpose ;
            owl:onProperty skos:inScheme ] .

org:resultingOrganization rdfs:domain org:ChangeEvent ;
    rdfs:range cccev:PublicOrganisation .

org:unitOf rdfs:domain org:OrganizationalUnit ;
    rdfs:range cccev:PublicOrganisation .

xhtml:prev rdfs:domain cccev:PublicOrganisation ;
    rdfs:range cccev:PublicOrganisation .

prov1:endedAtTime a owl:FunctionalProperty ;
    rdfs:domain org:ChangeEvent ;
    rdfs:range xsd:dateTime .

time:TemporalEntity a owl:Class .

org:ChangeEvent a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty prov1:endedAtTime ] .

cccev:PublicOrganisation a owl:Class .

[] a owl:AllDisjointClasses ;
    owl:members ( cccev:PublicOrganisation org:OrganizationalUnit ) .

[] a owl:AllDisjointClasses ;
    owl:members ( cccev:PublicOrganisation org:OrganizationalUnit ) .

