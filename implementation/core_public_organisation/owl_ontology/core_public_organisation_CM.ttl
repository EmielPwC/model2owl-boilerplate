cccev:ImageObject a owl:Class ;
    skos:definition "A logo to describe / represent the PublicOrganization."@en ;
    skos:prefLabel "Image object"@en .

cccev:PublicOrganisation a owl:Class ;
    rdfs:subClassOf org:Organization ;
    skos:definition "An Organization that is defined as being part of the public sector by a legal framework at any level."@en ;
    skos:prefLabel "Public organisation"@en .

cccev:ReferenceFramework a owl:Class ;
    skos:definition "Legislation or official policy from which Requirements are derived."@en ;
    skos:prefLabel "Reference framework"@en .

cccev:contactPoint a owl:ObjectProperty ;
    skos:definition "The main contact information of the resource. (cpov:PublicOrganisation -> cv:ContactPoint (+cpv:contactPoint))"@en ;
    skos:prefLabel "Contact point"@en .

cccev:frequency a owl:ObjectProperty ;
    skos:definition "The recurrence of an instant or period. (time:TemporalEntity -> cpov:frequency (+cpov:frequency))"@en ;
    skos:prefLabel "Frequency"@en .

cccev:hasFormalFramework a owl:ObjectProperty ;
    skos:prefLabel "Has formal framework"@en .

cccev:implements a owl:ObjectProperty ;
    skos:prefLabel "Implements"@en .

cccev:logo a owl:ObjectProperty ;
    skos:prefLabel "Logo"@en .

cccev:openingHours a owl:ObjectProperty ;
    skos:definition "The time at which the Contact Point is normally available. (cv:ContactPoint -> time:TemporalEntity (+cpov:openingHours))"@en ;
    skos:prefLabel "Opening hours"@en .

cccev:specialOpeningHoursSpecification a owl:ObjectProperty ;
    skos:definition "The time during which the Contact Point is not available. (cv:ContactPoint -> time:TemporalEntity (+cpov:specialOpeningHoursSpecification))"@en ;
    skos:prefLabel "Special opening hours specification"@en .

at-voc:atu-type skos:editorialNote "the URI in core voc is wrong; a dataset URI instead the conceptScheme URI."@en .

dcterms:description a owl:DatatypeProperty ;
    skos:definition "The textual description of the Public Organization. (cpov:PublicOrganisation) A textual explanation of the event. (org:ChangeEvent) A textual representation of the Temporal Entity. (time:TemporalEntity)"@en ;
    skos:prefLabel "Description"@en .

dcterms:identifier a owl:DatatypeProperty ;
    skos:definition "An unambiguous reference to a Reference Framework. (cccev:ReferenceFramework)"@en ;
    skos:prefLabel "Identifier"@en .

dcterms:spatial a owl:ObjectProperty ;
    skos:definition "This property links an Organization to the Administrative Region(s) that it covers. (cpov:PublicOrganisation -> at-voc:atu-type (+dct:spatial))"@en ;
    skos:prefLabel "Spatial"@en .

skos:altLabel a owl:DatatypeProperty ;
    skos:definition "An alternative or informal name(s) of a Public Organization, irrespective of language. (cpov:PublicOrganisation)"@en ;
    skos:prefLabel "Alt label"@en .

skos:prefLabel a owl:DatatypeProperty ;
    skos:definition "A preferred label is used to provide the primary, legally recognised name of the Public Organization, as defined in the ORG Ontology. (cpov:PublicOrganisation)"@en ;
    skos:prefLabel "Pref label"@en .

time:TemporalEntity a owl:Class ;
    skos:definition "A temporal interval or instant."@en ;
    skos:prefLabel "Temporal entity"@en .

adms:identifier a owl:ObjectProperty ;
    skos:prefLabel "Identifier"@en .

locn:address a owl:ObjectProperty ;
    skos:definition "The Address of a Public Organization. (cpov:PublicOrganisation -> locn:Address (+locn:address))"@en ;
    skos:prefLabel "Address"@en .

org:ChangeEvent a owl:Class ;
    skos:definition "An event which resulted in a major change to an Organization such as a merger or complete restructuring."@en ;
    skos:prefLabel "Change event"@en .

org:OrganizationalUnit a owl:Class ;
    rdfs:subClassOf org:Organization ;
    skos:definition "A subdivision of Organization which is part of this Organization."@en ;
    skos:prefLabel "Organizational unit"@en .

org:changedBy a owl:ObjectProperty ;
    skos:prefLabel "Changed by"@en .

org:classification a owl:ObjectProperty ;
    skos:definition "The categorisation of the Public Organization. (cpov:PublicOrganisation -> org:classification (+org:classification))"@en ;
    skos:prefLabel "Classification"@en .

org:hasMember a owl:ObjectProperty ;
    skos:prefLabel "Has member"@en .

org:hasSubOrganization a owl:ObjectProperty ;
    skos:prefLabel "Has sub organization"@en .

org:hasUnit a owl:ObjectProperty ;
    skos:prefLabel "Has unit"@en .

org:memberOf a owl:ObjectProperty ;
    skos:prefLabel "Member of"@en .

org:originalOrganization a owl:ObjectProperty ;
    skos:prefLabel "Original organization"@en .

org:purpose a owl:ObjectProperty ;
    skos:definition "This property describes the reasons of existence of an Organization. (cpov:PublicOrganisation -> org:purpose (+org:purpose))"@en ;
    skos:prefLabel "Purpose"@en .

org:resultedFrom a owl:ObjectProperty ;
    skos:prefLabel "Resulted from"@en .

org:resultingOrganization a owl:ObjectProperty ;
    skos:prefLabel "Resulting organization"@en .

org:subOrganizationOf a owl:ObjectProperty ;
    skos:prefLabel "Sub organization of"@en .

org:unitOf a owl:ObjectProperty ;
    skos:prefLabel "Unit of"@en .

foaf:Document a owl:Class ;
    skos:definition "A self-contained collection of information in a readable format."@en ;
    skos:prefLabel "Document"@en .

foaf:homepage a owl:ObjectProperty ;
    skos:definition "The official home page of the Public Organization. (cpov:PublicOrganisation -> foaf:Document (+foaf:homepage))"@en ;
    skos:prefLabel "Homepage"@en .

xhtml:next a owl:ObjectProperty ;
    skos:prefLabel "Next"@en .

xhtml:prev a owl:ObjectProperty ;
    skos:prefLabel "Prev"@en .

prov1:endedAtTime a owl:DatatypeProperty ;
    skos:definition "The time instant when the state of the organisation update was terminated (org:ChangeEvent)"@en ;
    skos:prefLabel "Ended at time"@en .

prov1:startedAtTime a owl:DatatypeProperty ;
    skos:definition "The time instant when the state of the organisation update was initiated. (org:ChangeEvent)"@en ;
    skos:prefLabel "Started at time"@en .

org:Organization a owl:Class ;
    skos:definition "A collection of people organized together into a community or other social, commercial or political structure. The group has some common purpose or reason for existence which goes beyond the set of people belonging to it and can act as an Agent. Organizations are often decomposable into hierarchical structures."@en ;
    skos:prefLabel "Organization"@en .

