@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix at-voc: <http://publications.europa.eu/resource/authority/> .
@prefix cccev: <http://data.europa.eu/m8g/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix person: <http://www.w3.org/ns/person#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cccev:birthDate rdfs:domain cccev:Person ;
    rdfs:range cccev:GenericDate .

cccev:contactPage rdfs:domain cccev:ContactPoint ;
    rdfs:range rdf:langString .

cccev:contactPoint rdfs:domain [ a owl:Class ;
            owl:unionOf ( cccev:Person cccev:PublicOrganisation ) ] ;
    rdfs:range cccev:ContactPoint .

cccev:cpv-restriction a owl:Ontology ;
    rdfs:label "Core Person Vocabulary - core restrictions"@en ;
    dcterms:created "2021-06-01"^^xsd:date ;
    dcterms:description "The Core Person Vocabulary core restrictions provides the restrictions and the inference-related specifications on the concepts and properties in the Core Person Vocabulary core."@en ;
    dcterms:issued "2024-03-26"^^xsd:date ;
    dcterms:license "Â© European Union, 2014. Unless otherwise noted, the reuse of the Ontology is authorised under the European Union Public Licence v1.2 (https://eupl.eu/)." ;
    dcterms:publisher "http://publications.europa.eu/resource/authority/corporate-body/PUBL" ;
    dcterms:title "Core Person Vocabulary - core restrictions"@en ;
    vann:preferredNamespacePrefix "cv" ;
    vann:preferredNamespaceUri "http://data.europa.eu/m8g/ontology#" ;
    rdfs:comment """This version is automatically generated from core_person_CM.xml on 
                2024-03-26""" ;
    rdfs:seeAlso <https://semiceu.github.io/Core-Person-Vocabulary/releases/2.1.0/> ;
    owl:imports cccev:,
        cccev:cpv,
        at-voc:,
        dcterms:,
        <http://www.opengis.net/ont/geosparql>,
        <http://www.w3.org/2004/02/skos/core>,
        <http://www.w3.org/2006/time>,
        <http://www.w3.org/ns/adms>,
        <http://www.w3.org/ns/locn>,
        org:,
        <http://www.w3.org/ns/person>,
        foaf:,
        <https://data.europa.eu/m8g/>,
        <https://www.w3.org/1999/xhtml/vocab>,
        <https://www.w3.org/ns/prov> ;
    owl:incompatibleWith "3.1.0" ;
    owl:priorVersion "http://data.europa.eu/m8g/cpv-restriction-3.1.0" ;
    owl:versionIRI cccev:cpv-restriction-4.0.0 ;
    owl:versionInfo "4.0.0" .

cccev:deathDate rdfs:domain cccev:Person ;
    rdfs:range cccev:GenericDate .

cccev:domicile rdfs:domain cccev:Person ;
    rdfs:range locn:Address .

cccev:email rdfs:domain cccev:ContactPoint ;
    rdfs:range rdfs:Literal .

cccev:identifies rdfs:domain adms:Identifier ;
    rdfs:range cccev:Person .

cccev:matronymicName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

cccev:openingHours rdfs:domain cccev:ContactPoint ;
    rdfs:range time:TemporalEntity .

cccev:patronymicName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

cccev:specialOpeningHoursSpecification rdfs:domain cccev:ContactPoint ;
    rdfs:range time:TemporalEntity .

cccev:telephone rdfs:domain cccev:ContactPoint ;
    rdfs:range rdfs:Literal .

dcterms:alternative rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

dcterms:conformsTo rdfs:domain adms:Identifier ;
    rdfs:range rdf:langString .

dcterms:creator rdfs:domain adms:Identifier ;
    rdfs:range foaf:Agent .

dcterms:identifier rdfs:domain cccev:Person,
        dcterms:Jurisdiction ;
    rdfs:range xsd:anyURI,
        adms:Identifier .

dcterms:issued rdfs:domain adms:Identifier ;
    rdfs:range xsd:date .

dcterms:title rdfs:domain foaf:Agent ;
    rdfs:range rdf:langString .

dcterms:type rdf:domain foaf:Agent ;
    rdfs:range skos:Concept .

rdfs:label rdfs:domain [ a owl:Class ;
            owl:unionOf ( adms:Identifier dcterms:Jurisdiction ) ] ;
    rdfs:range rdf:langString .

skos:notation rdfs:domain adms:Identifier ;
    rdfs:range rdfs:Literal .

adms:identifier rdfs:domain cccev:PublicOrganisation ;
    rdfs:range adms:Identifier .

adms:schemeAgency rdfs:domain adms:Identifier ;
    rdfs:range rdf:langString .

person:birthName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

person:citizenship rdfs:domain cccev:Person ;
    rdfs:range dcterms:Jurisdiction .

person:countryOfBirth rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:countryOfDeath rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:placeOfBirth rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:placeOfDeath rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:residency rdfs:domain cccev:Person ;
    rdfs:range dcterms:Jurisdiction .

foaf:familyName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

foaf:givenName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

foaf:name rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

cccev:gender a owl:Class ;
    rdfs:domain cccev:Person ;
    rdfs:range skos:Concept ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue cccev:gender ;
            owl:onProperty skos:inScheme ] .

cccev:sex a owl:Class ;
    rdfs:domain cccev:Person ;
    rdfs:range skos:Concept ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue cccev:sex ;
            owl:onProperty skos:inScheme ] .

cccev:type a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue cccev:type ;
            owl:onProperty skos:inScheme ] .

dcterms:Jurisdiction a owl:Class .

adms:Identifier a owl:Class .

