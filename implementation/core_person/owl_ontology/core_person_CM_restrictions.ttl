cccev:birthDate rdfs:domain cccev:Person ;
    rdfs:range cccev:GenericDate .

cccev:contactPage rdfs:domain cccev:ContactPoint ;
    rdfs:range rdf:langString .

cccev:contactPoint rdfs:domain [ a owl:Class ;
            owl:unionOf ( cccev:Person cccev:PublicOrganisation ) ] ;
    rdfs:range cccev:ContactPoint .

cccev:deathDate rdfs:domain cccev:Person ;
    rdfs:range cccev:GenericDate .

cccev:domicile rdfs:domain cccev:Person ;
    rdfs:range locn:Address .

cccev:email rdfs:domain cccev:ContactPoint ;
    rdfs:range rdfs:Literal .

cccev:gender rdf:domain cccev:Person ;
    rdfs:range skos:Concept .

cccev:identifies rdfs:domain adms:Identifier ;
    rdfs:range cccev:Person .

cccev:matronymicName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

cccev:openingHours rdfs:domain cccev:ContactPoint ;
    rdfs:range time:TemporalEntity .

cccev:patronymicName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

cccev:specialOpeningHoursSpecification rdfs:domain cccev:ContactPoint ;
    rdfs:range time:TemporalEntity .

cccev:telephone rdfs:domain cccev:ContactPoint ;
    rdfs:range rdfs:Literal .

dcterms:alternative rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

dcterms:conformsTo rdfs:domain adms:Identifier ;
    rdfs:range rdf:langString .

dcterms:creator rdfs:domain adms:Identifier ;
    rdfs:range foaf:Agent .

dcterms:identifier rdfs:domain cccev:Person,
        dcterms:Jurisdiction ;
    rdfs:range xsd:anyURI,
        adms:Identifier .

dcterms:issued rdfs:domain adms:Identifier ;
    rdfs:range xsd:date .

dcterms:title rdfs:domain foaf:Agent ;
    rdfs:range rdf:langString .

dcterms:type rdfs:domain foaf:Agent ;
    rdfs:range skos:Concept .

rdfs:label rdfs:domain [ a owl:Class ;
            owl:unionOf ( adms:Identifier dcterms:Jurisdiction ) ] ;
    rdfs:range rdf:langString .

skos:notation rdfs:domain adms:Identifier ;
    rdfs:range rdfs:Literal .

adms:identifier rdfs:domain cccev:PublicOrganisation ;
    rdfs:range adms:Identifier .

adms:schemeAgency rdfs:domain adms:Identifier ;
    rdfs:range rdf:langString .

person:birthName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

person:citizenship rdfs:domain cccev:Person ;
    rdfs:range dcterms:Jurisdiction .

person:countryOfBirth rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:countryOfDeath rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:placeOfBirth rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:placeOfDeath rdfs:domain cccev:Person ;
    rdfs:range dcterms:Location .

person:residency rdfs:domain cccev:Person ;
    rdfs:range dcterms:Jurisdiction .

foaf:familyName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

foaf:givenName rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

foaf:name rdfs:domain cccev:Person ;
    rdfs:range rdf:langString .

dcterms:Jurisdiction a owl:Class .

adms:Identifier a owl:Class .

